{% block sw_cms_sidebar_block_overview_category_options %}
    {% parent() %}

    <option
        v-if="!isBlogDetail"
        value="werkl-blog"
    >Blog</option>
{% endblock %}
{% block sw_cms_sidebar_page_settings_type_field_options %}
    {% parent() %}

    <option
        value="blog_detail"
        :disabled="!isBlogDetail"
    >
        {{ $t('sw-cms.detail.label.pageTypeBlog') }}
    </option>
{% endblock %}
{% block sw_cms_sidebar_page_settings_type_field %}
    {% parent() %}

    <mt-text-field
        v-model="page.toggle"
        :label="$t('sw-cms.detail.label.pageName')"
        :placeholder="placeholder(page, 'name')"
        :error="pageNameError"
    />
{% endblock %}